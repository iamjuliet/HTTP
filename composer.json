{
    "name": "aplus/http",
    "type": "library",
    "description": "Aplus Framework HTTP Library",
    "keywords": [
        "http",
        "anti-csrf",
        "csrf",
        "message",
        "request",
        "response",
        "url",
        "cookie",
        "user-agent",
        "download",
        "rest"
    ],
    "homepage": "https://docs.aplus-framework.com/guides/libraries/http/",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "aplus/helpers": "^2.2"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.9",
        "ergebnis/composer-normalize": "^2.15",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\HTTP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\HTTP\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/http/issues",
        "wiki": "https://gitlab.com/aplus-framework/libraries/http/wikis/home",
        "source": "https://gitlab.com/aplus-framework/libraries/http",
        "docs": "https://docs.aplus-framework.com/guides/libraries/http/"
    },
    "funding": [
        {
            "type": "other",
            "url": "https://aplus-framework.com/sponsor"
        }
    ]
}
